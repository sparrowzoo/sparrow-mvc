<taglib xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
    http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
        version="2.0">
    <!-- 定义函数版本 -->
    <tlib-version>1.0</tlib-version>
    <!-- 定义函数名称 -->
    <short-name>sparrowEL</short-name>

    <uri>http://www.sparrowzoo.com/el</uri>
    <!-- 定义第一个函数 -->
    <function>
        <name>i18n</name>
        <function-class>com.sparrow.utility.Config</function-class>
        <function-signature>java.lang.String getLanguageValue(java.lang.String)</function-signature>
    </function>

    <function>
        <name>config</name>
        <function-class>com.sparrow.utility.Config</function-class>
        <function-signature>java.lang.String getValue(java.lang.String)</function-signature>
    </function>

    <function>
        <name>wrap_enter_p</name>
        <function-class>com.sparrow.utility.StringUtility</function-class>
        <function-signature>java.lang.String wrap(java.lang.String)</function-signature>
    </function>

    <function>
        <name>wrap_enter</name>
        <function-class>com.sparrow.utility.StringUtility</function-class>
        <function-signature>java.lang.String wrap(java.lang.String,java.lang.String))</function-signature>
    </function>
    <function>
        <name>wrap</name>
        <function-class>com.sparrow.utility.StringUtility</function-class>
        <function-signature>java.lang.String wrap(java.lang.String))</function-signature>
    </function>

    <function>
        <name>substring</name>
        <function-class>com.sparrow.utility.StringUtility</function-class>
        <function-signature>java.lang.String subStringByByte(java.lang.String,int)</function-signature>
    </function>
</taglib>